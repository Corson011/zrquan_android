<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="4.0dip" />
            <solid android:color="#ff028ce6" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <solid android:color="#fff6f6f6" />
            <padding android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px" />
            <stroke android:width="1.0px" android:color="#ffcecece" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="4.0dip" />
            <solid android:color="#ff02baff" />
        </shape>
    </item>
</selector>